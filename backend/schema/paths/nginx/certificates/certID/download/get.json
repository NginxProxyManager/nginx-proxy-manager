{
	"operationId": "downloadCertificate",
	"summary": "Downloads a Certificate",
	"tags": ["certificates"],
	"security": [
		{
			"bearerAuth": ["certificates.manage"]
		}
	],
	"parameters": [
		{
			"in": "path",
			"name": "certID",
			"description": "Certificate ID",
			"schema": {
				"type": "integer",
				"minimum": 1
			},
			"required": true,
			"example": 1
		}
	],
	"responses": {
		"200": {
			"description": "200 response",
			"content": {
				"application/zip": {
					"schema": {
						"type": "string",
						"format": "binary"
					}
				}
			}
		}
	}
}
